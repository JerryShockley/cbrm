extends ../list_layout.pug

block content
  header.center
    h1= title
  main.main.top-margin
    table#mapping-table.display(style=`width:100%`)
      thead
        tr
          th Brand
          th Order
          th X
          th Y
          th Duration
          th Respondent ID
          th Project ID
          th Created
      tfoot
        tr
          th Brand
          th Order
          th X
          th Y
          th Duration
          th Respondent ID
          th Project ID
          th Created

    .flex.top-margin
      button.button.red(type=`button` onclick=`adminUrl()`) Close

  include ../includes/scripts.pug
  script(src=`/javascripts/mapping_list.js`)
  script.
    function adminUrl() {
      window.location.assign(`${window.location.origin}/admin/3355`)
    }
