  "devDependencies": {
    "reload": "^3.1.1",
    "eslint": "^7.19.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "pm2": "^4.5.4",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4"
