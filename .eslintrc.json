{
  "extends": "airbnb-base",
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "worker": true
  },
  "parserOptions": {
    "ecmaVersion": 10,
    "sourceType": "module"
  },
  "rules": {
    "quotes": ["error", "backtick"],
    "semi": ["error", "never"],
    "no-use-before-define": "off",
    "no-undef": "off",
    "no-unused-vars": "warn",
    "class-methods-use-this": "warn",
    "object-curly-newline": "off",
    "no-path-concat": "off",
    "comma-dangle": "off",
    "import/no-dynamic-require": "off",
    "prefer-template":"warn",
    "arrow-body-style": ["error", "always"]
  }
}
